<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twt.mapper.StatusMapper">

    <update id="updateApplyStatus">
        update tb_status set apply = #{status}
    </update>

    <update id="updateConfirmStatus">
        update tb_status set confirm = #{status}
    </update>
</mapper>
